{
  "name": "pxt-stm32-iot-node",
  "version": "0.12.11",
  "description": "ST IoT-Node for Microsoft MakeCode",
  "public": true,
  "keywords": [
    "JavaScript",
    "education",
    "maker",
    "pxt",
    "MakeCode",
    "Microsoft"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/pxt-stm32-iot-node.git"
  },
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/Microsoft/pxt-stm32-iot-node.git#readme",
  "files": [
    "README.md",
    "pxtarget.json",
    "targetconfig.json",
    "built/*.js",
    "built/*.json",
    "built/*.d.ts",
    "sim/public",
    "docs/*.md",
    "docs/*/*.md",
    "docs/*/*/*.md",
    "docs/static/*.svg",
    "docs/static/*.png",
    "docs/static/libs/*.png",
    "docs/static/fonts/*",
    "docs/static/icons/favicon.ico"
  ],
  "devDependencies": {
    "typescript": "2.6.1",
    "semantic-ui-less": "2.2.14",
    "@types/bluebird": "2.0.33",
    "@types/jquery": "3.2.16",
    "@types/marked": "0.3.0",
    "@types/node": "8.0.53",
    "@types/web-bluetooth": "0.0.4"
  },
  "dependencies": {
    "glob-exec": "^0.1.1",
    "pxt-common-packages": "6.4.5",
    "pxt-core": "5.4.13",
    "serialport": "7.1.4",
    "svgo": "^1.1.1",
    "webusb": "^1.1.0"
  },
  "scripts": {
    "start": "node node_modules/pxt-core/built/pxt.js serve --cloud --no-browser -h \"0.0.0.0\" -w 3233 -p 3232",
    "test": "node node_modules/pxt-core/built/pxt.js travis",
    "svgo": "glob-exec --foreach \"**/boardhd.svg\" -- \"svgo --config=svgo.yml {{file}} -o {{file.dir}}/board.svg \""
  }
}
